[general]
status_path = "~/.local/share/vdirsyncer/status/"

[pair contacts]
a = "contacts_local"
b = "contacts_remote"
collections = null

[storage contacts_local]
type = "filesystem"
path = "~/permanent/contacts/"
fileext = ".vcf"

[storage contacts_remote]
type = "carddav"
url = "https://cal.blinry.org/blinry/025d65a1-0209-1c28-b394-38ea87a4ef08/"
username = "blinry"
password.fetch = ["command", "~/.config/vdirsyncer/get-password.sh"]

[pair calendar]
a = "calendar_local"
b = "calendar_remote"
collections = ["from a", "from b"]
metadata = ["color", "displayname"]

[storage calendar_local]
type = "filesystem"
path = "~/permanent/calendar/"
fileext = ".ics"

[storage calendar_remote]
type = "caldav"
url = "https://cal.blinry.org"
username = "blinry"
password.fetch = ["command", "~/.config/vdirsyncer/get-password.sh"]
