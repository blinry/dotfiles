# List of modifiers:
#   Release, Control, Shift, Mod1 (Alt), Mod2 (NumLock),
#   Mod3 (CapsLock), Mod4, Mod5 (Scroll).

# "amixer set Master toggle"
#     XF86AudioMute
# "amixer sset Master 5%-"
#     XF86AudioLowerVolume
# "amixer sset Master 5%+"
#     XF86AudioRaiseVolume

"mpc toggle"
    XF86AudioPlay
"mpc stop"
     XF86AudioStop
"mpc prev"
    XF86AudioPrev
"mpc next"
    XF86AudioNext

"firefox"
    F1 + Mod1
"xterm -e mutt"
    F2 + Mod1
"pidgin"
    F3 + Mod1
